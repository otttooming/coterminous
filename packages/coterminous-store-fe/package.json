  "scripts": {
    "dev": "npm run get:omniva && npm run get:smartpost && nodemon",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "test": "jest --no-cache",
    "test:watch": "jest --watch",
    "sprite": "svg-sprite-generate -d ./static/assets -o ./static/sprite.svg",
    "get:omniva": "curl https://www.omniva.ee/locations.json -o ./static/shipping/omniva.json",
    "get:smartpost": "curl https://uus.smartpost.ee/places.json -o ./static/shipping/smartpost.json",
    "gen:graphql": "gql-gen"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "verbose": true,
    "bail": false,
    "collectCoverage": true
